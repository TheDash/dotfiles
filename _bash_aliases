alias df='df -H'
alias du1='du -ch --max-depth=1'
alias duh='du -chs ./*'
alias fn='find . -name'
alias hi='history | tail -20'
alias flushdns='sudo dscacheutil -flushcache'

alias mount.prk='sshfs wwoodall@prk:/ ~/mnt/prk -oauto_cache,reconnect,defer_permissions,negative_vncache,volname=prk'
alias umount.prk='umount ~/mnt/prk'


